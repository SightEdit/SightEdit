{
  "name": "@sightedit/graphql-server",
  "version": "2.0.0-alpha.1",
  "description": "GraphQL server for SightEdit with Apollo Server integration",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "type-check": "tsc --noEmit",
    "start": "node dist/index.js"
  },
  "keywords": [
    "sightedit",
    "graphql",
    "apollo-server",
    "subscriptions",
    "real-time"
  ],
  "author": "SightEdit Team",
  "license": "MIT",
  "peerDependencies": {
    "@sightedit/core": "^2.0.0-alpha.1"
  },
  "dependencies": {
    "@apollo/server": "^4.10.0",
    "graphql": "^16.8.1",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.14.3",
    "ws": "^8.16.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "body-parser": "^1.20.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/ws": "^8.5.10",
    "typescript": "^5.3.0",
    "tsup": "^8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sightedit/sightedit.git",
    "directory": "packages/graphql-server"
  }
}
