{
  "name": "@sightedit/server-sdk",
  "version": "2.0.0-alpha.1",
  "description": "Custom backend SDK for SightEdit - Build your own CMS adapter",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "examples"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "sightedit",
    "backend",
    "sdk",
    "adapter",
    "custom-cms"
  ],
  "author": "SightEdit Team",
  "license": "MIT",
  "peerDependencies": {
    "@sightedit/core": "^2.0.0-alpha.1"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "tsup": "^8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sightedit/sightedit.git",
    "directory": "packages/server-sdk"
  }
}
